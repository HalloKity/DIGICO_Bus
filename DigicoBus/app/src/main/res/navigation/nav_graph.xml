<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/CommuteFragment"
    android:id="@+id/nav_graph">

    <fragment
        android:id="@+id/CommuteFragment"
        android:name="com.kt.digicobus.fragment.commute.CommuteFragment"
        android:label="fragment_commute"
        tools:layout="@layout/fragment_commute" >
        <action
            android:id="@+id/action_CommuteFragment_to_ReservationConfirmFragment"
            app:destination="@id/ReservationConfirmFragment" />
    </fragment>
    <fragment
        android:id="@+id/ReservationConfirmFragment"
        android:name="com.kt.digicobus.fragment.ReservationConfirmFragment"
        android:label="fragment_reservation_confirm"
        tools:layout="@layout/fragment_reservation_confirm" >
    </fragment>
    <fragment
        android:id="@+id/ShuttleFragment"
        android:name="com.kt.digicobus.fragment.shuttle.ShuttleFragment"
        android:label="fragment_shuttle"
        tools:layout="@layout/fragment_shuttle" />
    <fragment
        android:id="@+id/CarPoolFragment"
        android:name="com.kt.digicobus.fragment.CarPoolFragment"
        android:label="fragment_car_pool"
        tools:layout="@layout/fragment_car_pool" />
    <fragment
        android:id="@+id/MoreFragment"
        android:name="com.kt.digicobus.fragment.MoreFragment"
        android:label="fragment_more"
        tools:layout="@layout/fragment_more" />
</navigation>